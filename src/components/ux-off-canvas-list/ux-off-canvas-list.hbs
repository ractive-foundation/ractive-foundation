<ul class="off-canvas-list {{class}}">
	{{#each this.items}}
		<li>
			{{#if this.href}}
				<a href="{{this.href}}"
					on-tap="menuSelect"
					{{#if this.target}}target="{{this.target}}"{{/if}}>
					{{{this.label}}}
				</a>
			{{else}}
				<label>{{{this.label}}}</label>
			{{/if}}
		</li>
	{{/each}}
</ul>
