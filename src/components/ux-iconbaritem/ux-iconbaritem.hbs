<a href="{{href}}" class="item {{class}}"
	tabindex="0" role="button"
	{{#if labels}}aria-labelledby="{{guid}}"{{/if}}
	{{#if arialabel}}aria-label={{arialabel}}{{/if}}
>

	{{#if src}}
		<img src="{{src}}">
	{{/if}}

	{{#if labels}}
		<label id="{{guid}}">
			{{#if label}}
				{{{label}}}
			{{else}}
				{{yield}}

			{{/if}}
		</label>
	{{/if}}

</a>